public List<sObject> getListOfsObject(List<Account> accounts, List<Contact> contacts)
{
    List<sObject> objects = new List<sObject>();

    if(accounts == null && contacts == null) {
        return objects;
    } else {
              
        if(Account != null) 
            for(Account acc :accounts) {
                objects.add(acc);
            }

        if(Contact != null)
            for(Contact con :contacts) {
                objects.add(con);
            } 

        return objects;
    }
}